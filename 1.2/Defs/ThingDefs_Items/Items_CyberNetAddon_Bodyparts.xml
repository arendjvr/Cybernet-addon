<?xml version="1.0" encoding="UTF-8"?>
<Defs>
	
	<!-- Networked (Bionic) Level Body Parts -->
	<ThingDef ParentName="CyberNetProsthetic">
		<defName>CNANetworkedLung</defName>
		<label>Networked Lung</label>
		<description>A networked prosthetic that offloads processing to remote processors. Cheaper than self contained bionic Lungs, and more efficient, but requires infrastructure to function.</description>
		<descriptionHyperlinks><RecipeDef>CNAInstallNetworkedLung</RecipeDef></descriptionHyperlinks>
		<statBases>
			<WorkToMake>30000</WorkToMake>
			<Mass>1.5</Mass>
		</statBases>
		<costList>
			<Plasteel>7</Plasteel>
			<ComponentSpacer>1</ComponentSpacer>
		</costList>
		<recipeMaker>
			<skillRequirements>
				<Crafting>10</Crafting>
			</skillRequirements>
			<researchPrerequisite>CNANetworkedOrgans</researchPrerequisite>
		</recipeMaker>
	</ThingDef>

