  <RecipeDef ParentName="CyberNetSurgery">
    <defName>CNAInstallAdvancedNetworkedLung</defName>
    <label>Install Advanced Networked Lung</label>
    <description>Install an Advanced Networked Lung</description>
    <jobString>Installing Advanced Networked Lung</jobString>
    <workAmount>2500</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CNAAdvancedNetworkedLung</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>CNAAdvancedNetworkedLung</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Lung</li>
    </appliedOnFixedBodyParts>
    <addsHediff>CNAAdvancedNetworkedLung</addsHediff>
  </RecipeDef>

