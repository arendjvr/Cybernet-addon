  <RecipeDef ParentName="CyberNetSurgery">
    <defName>CNInstallAdvancedNetworkedLung</defName>
    <label>Install Advanced Networked Lung</label>
    <description>Install an Advanced Networked Lung</description>
    <jobString>Installing Advanced Networked Lung</jobString>
    <workAmount>2500</workAmount>
    <ingredients>
      <li>
        <filter>
          <categories>
            <li>Medicine</li>
          </categories>
        </filter>
        <count>2</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>CNAdvancedNetworkedLung</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <categories>
        <li>Medicine</li>
      </categories>
      <thingDefs>
        <li>CNAdvancedNetworkedLung</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
      <li>Lung</li>
    </appliedOnFixedBodyParts>
    <addsHediff>CNAdvancedNetworkedLung</addsHediff>
  </RecipeDef>

