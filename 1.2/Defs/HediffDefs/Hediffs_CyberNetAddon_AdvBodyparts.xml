<?xml version="1.0" encoding="utf-8" ?>
<Defs>
        <HediffDef ParentName="CNHediffProsthetic">
        <defName>CNAdvancedNetworkedLung</defName>
        <label>Advanced Networked Lung</label>
        <labelNoun>An Advanced Networked Lung</labelNoun>
        <description>An Advanced Artificial lung that requires a network to function properly. If the connection with the network is lost, the lung will start to fail and weaken the host.</description>
        <spawnThingOnRemoved>CNAdvancedNetworkedLung</spawnThingOnRemoved>
        <comps>
            <li Class="CyberNet.HediffCompPropertiesCyberNetImplant">
                <compClass>CyberNet.HediffCompCyberNetImplant</compClass>
                <DefaultSeverity>1</DefaultSeverity>
                <BandwidthUsage>0.75</BandwidthUsage>
                <ProcessorUsage>0.25</ProcessorUsage>
            </li>
        </comps>
        <stages>
            <li>
                <label>disconnected</label>
                <minSeverity>0</minSeverity>
                    <partEfficiencyOffset>0</partEfficiencyOffset>
            </li>
            <li>
                <label>throttled</label>
                <minSeverity>0.5</minSeverity>
                    <partEfficiencyOffset>0.10</partEfficiencyOffset>
            </li>
            <li>
                <label>sluggish</label>
                <minSeverity>0.75</minSeverity>
                    <partEfficiencyOffset>0.15</partEfficiencyOffset>
            </li>
            <li>
                <label>connected</label>
                <minSeverity>1</minSeverity>
                    <partEfficiencyOffset>0.20</partEfficiencyOffset>
            </li>
            <li>
                <label>overclocked</label>
                <minSeverity>1.15</minSeverity>
                    <partEfficiencyOffset>0.30</partEfficiencyOffset>
            </li>
            <li>
                <label>supercharged</label>
                <minSeverity>1.3</minSeverity>
                    <partEfficiencyOffset>0.35</partEfficiencyOffset>
            </li>
        </stages>
    </HediffDef>

